# Process with autom4te to create an -*- Autotest -*- test suite.


# ------------------------------
#

AT_INIT([expr-test])
# AT_COPYRIGHT([])

# AT_TESTED([expr-test])

# Usage: _AT_TEST_*(<expr source>, <baseline file>)

m4_define([_AT_DODS_TEST],   
[AT_BANNER([Test $1 $2])
AT_SETUP([DODS $1])
AT_KEYWORDS([expr])
AT_CHECK([$srcdir/expr-test -p < $1], [], [stdout], [ignore])
AT_CHECK([diff -b -B $2 stdout], [], [ignore],[],[])
AT_CLEANUP])

m4_define([AT_DODS_RESPONSE_TEST],
[AT_BANNER([EXPR response for $1.])
_AT_DAS_TEST([$srcdir/expr-testsuite/$1], [$srcdir/expr-testsuite/$1.base])
])

